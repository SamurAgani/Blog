@using EntityLayer.Concrete
@model List<Natification>
@{
    ViewData["Title"] = "AllNatifation";
    Layout = "~/Views/Shared/WriterLayout.cshtml";
}

<h1>Notifications</h1>

<table class="table table-bordered">
    <tr>
        <th>#</th>
        <th>Notification</th>
        <th>Date</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.NatifationDetails</td>
            <td>@item.NatificationDate.ToShortDateString()</td>
            <td></td>
        </tr>
    }
</table>