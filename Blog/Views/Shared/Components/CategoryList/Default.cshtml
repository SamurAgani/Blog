@model IEnumerable<EntityLayer.Concrete.Category>
<div class="tech-btm">
    <h4>Categories</h4>
    <ul class="list-group single">
        @foreach (var item in Model)
        {
            <li class="list-group-item d-flex justify-content-between align-items-lg-center">
                @item.CategoryName
            </li>

        }
    </ul>
</div>